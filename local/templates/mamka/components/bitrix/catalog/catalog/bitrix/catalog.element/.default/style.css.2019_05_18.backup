#product-tabs {
    margin: 45px 0px 0;
    width: 100%;
}

#product-tabs #product-tabs-buttons {
    border-bottom: 4px solid #d5afc3;
}

#product-tabs #product-tabs-buttons .product-tab-button {
    display: inline-block;
    color: #344d76;
    font-size: 24px;
    line-height: 45px;
    font-family: "FuturaRoundBold", "Roboto", sans-serif;
    color: #344d76;
    padding: 0 25px;
    cursor: pointer;
    transition: color .2s ease-in;
}

#product-tabs #product-tabs-buttons .product-tab-button:hover {
    color: #e01d7b
}

#product-tabs #product-tabs-buttons .product-tab-button.active {
    background: #d5afc3;
    color: #fff;
    position: relative;
}

#product-tabs #product-tabs-buttons .product-tab-button.active:before {
    width: 100%;
    height: 15px;
    position: absolute;
    top: -15px;
    left: 0;
    content: "";
    display: block;
    background: linear-gradient(to top right, #d5afc3 48%, transparent 52%), linear-gradient(to top right, transparent 50%, transparent 50%)
}

#product-tabs-contents {
    padding: 30px 0;
    color: #00104b;
}

#product-tabs-contents .product-tab {
    display: none;
}

#product-tabs-contents .product-tab.active {
    display: block
}

#product-tabs-contents .product-tab .char-table {
    width: 100%;
    border-collapse: collapse;
}

#product-tabs-contents .product-tab .char-table td {
    width: 25%;
    height: 44px;
    font-size: 14px;
    padding: 0 25px;
    border: 1px solid #e6e7ed
}
