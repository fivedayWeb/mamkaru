.modal_window
{
	position: fixed;
	top: 0;
	left: 0;
	width: 100vw;
	height: 100vh;
	z-index: 900;
}
.modal_window .flex
{
	justify-content: center;
	align-items: center;
}
.modal_bg
{
	background-color: rgba(0,0,0,0.7);
	width: 100%;
	height: 100%;
	cursor: pointer;
}
.modal_content
{
	background-color: #fff;
	position: relative;
	cursor: default;
	cursor: initial;
	max-width: 90vw;
	width: 90vw;
	max-height: 90vh;
}
.modal_close
{
	position: absolute;
	right: 0;
	top: 0;
	padding: 5px;
	cursor: pointer;
	font-size: 40px;
    line-height: 20px;
}
.modal_body
{
	padding: 25px 25px;
	box-sizing: border-box;
}
.modal-images-container .slider-item
{
	width: calc(90vw - 50px);
	height: calc(90vh - 50px);
	max-width: calc(90vw - 50px);
    max-height: calc(90vh - 50px);
}
.modal-images-container .owl-controls {
    height: 0;
}
.modal-images-container .owl-controls .owl-nav .owl-prev,
.modal-images-container .owl-controls .owl-nav .owl-next
{
	position: absolute;
    background: url("images/slider-arrow.png") no-repeat center center;
    height: 100%;
    width: 5%;
    margin: auto;
    top: 0;
    bottom: 0;
    opacity: .5;
    transition: all .2s ease-in;
}
.modal-images-container .owl-controls .owl-nav .owl-prev {
    left: 0;
}
.modal-images-container .owl-controls .owl-nav .owl-next {
    right: 0;
    transform: rotate(180deg);
}
.modal-images-container .owl-controls .owl-nav .owl-prev:hover,
.modal-images-container .owl-controls .owl-nav .owl-next:hover 
{
    opacity: 1;
}
.modal-images-container .owl-controls .owl-dots {
    display: none;
}
.modal-images-container.owl-carousel .owl-item img,
.modal-images-container .slider-item img
{
	width: auto;
	max-width: 100%;
	height: auto;
	max-height: 100%;
	object-fit: contain;
}

#product-gallery {
    width: 42%;
    margin-right: 3%;
}

#product-gallery #big-product-image {
    width: calc(100% - 2px);
    height: auto;
    border: 1px solid #ecebeb;
    border-radius: 3px;
    display: block;
    padding-top: 100%;
    position: relative;
}

#product-gallery #big-product-image img {
    position: absolute;
