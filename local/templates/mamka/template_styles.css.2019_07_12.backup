    #header #mid-header #menu-button.active span.line3 {
        transform: rotate(-45deg);
        top: 12px
    }
    #header #mid-header #cart-like-block {
        width: 100px;
        margin: 0 20px 0 0;
    }
    #header #mid-header #cart-like-block a#like-header-link {
        width: 50px;
        height: 50px;
        margin: 0;
    }
    #header #mid-header #cart-like-block a#like-header-link svg {
        width: 50px;
        height: 50px
    }
    #header #mid-header #cart-like-block a#like-header-link span {
        display: none
    }
    #header #mid-header #cart-like-block a#cart-header-link {
        width: 50px;
        height: 50px;
    }
    #header #mid-header #cart-like-block a#cart-header-link svg {
        width: 50px;
        height: 50px
    }
    #header #mid-header #cart-like-block a#cart-header-link span {
        display: none
    }

    #header #mid-header #mid-nav-block #search-form input {
        right: 70px;
        height: 30px;
        line-height: 30px;
        width: calc(100% - 92px)
    }
    #header #bottom-header {
        padding: 0 20px;
    }
    #header #bottom-header a {
        line-height: 39px;
        padding: 0;
        width: auto;
        display: inline-block;
        margin: 0 20px 0 0;
    }
    #header #bottom-header a .header-icon {
        display: none
    }
    /*----------------/header-----------------*/
    /*----------------footer-----------------*/
    #footer .footer-contacts-block {
        margin: 0 0 0 20px;
        width: 190px;
    }
    #footer #company-info {
        margin: 0;
        min-width: 230px;
    }
    #footer #company-info #footer-call-you {
        margin-top: 45px;
        font-size: 12px
    }
    #footer #company-info #footer-tel a {
        font-size: 28px
    }
    #footer #company-info #footer-social {
        font-size: 12px;
        margin-top: 0;
    }
    #footer #company-info #footer-social a {
        width: 29px;
        height: 29px;
    }
    #footer #company-info #footer-social a.google {
        background-size: 21px 21px
    }
    #footer #company-info #footer-social a.vk {
        background-size: 21px 21px
    }
    #footer #company-info #footer-social a.ok {
        background-size: 21px 21px
    }
    #footer #company-info #footer-social a.mail {
        background-size: 21px 21px
    }
    #footer #footer-nav {
        float: right;
        width: calc(100% / 12 * 5);
        margin: 0 20px 0 0;
    }
    #footer #footer-nav .footer-nav-column {
        width: auto;
        float: none;
    }
    #footer #footer-nav .footer-nav-column:not(.mobile-show) {
        display: none
    }
    #footer #footer-nav .footer-nav-column h2 {
        font-size: 12px;
        margin: 0 0 0 20px
    }
    #footer #footer-nav .footer-nav-column ul li {
        font-size: 12px;
        line-height: 17px;
        float: left;
        margin: 0 0 0 20px;
        width: calc(50% - 20px)
    }
    #footer #bottom-footer .made-in {
        font-size: 14px
    }
    /*----------------/footer-----------------*/
}

@media screen and (max-width:767px) {
