    transition: all .2s ease-in;
}

.count-picker .count-button:hover {
    background: #e01d7b
}

.count-picker .count-counter {
    display: inline-block;
    width: 15px
}

.checkbox-ui {
    display: inline-block;
    font-weight: 300;
    cursor: pointer;
    overflow: hidden;
    margin: 0 0 9px;
    line-height: 16px;
    color: #00104b;
}

.checkbox-ui input[type="checkbox"]:checked + div,
.checkbox-ui input[type="checkbox"]:checked + .d-block
{
    font-weight: 500
}

.checkbox-ui input[type="checkbox"]:checked + div.checkbox-item:after,
.checkbox-ui input[type="checkbox"]:checked + .d-block.checkbox-item:after
{
    transform: translateY(0)
}

.checkbox-ui:hover {
    font-weight: 500;
}

.checkbox-ui:hover .checkbox-item {
    border: 1px solid #00104b
}

.checkbox-ui input {
    display: none
}

.checkbox-ui div,
.checkbox-ui .d-block
{
    display: inline-block;
    font-size: 14px;
    line-height: 16px;
    vertical-align: middle;
}

.checkbox-ui div.checkbox-item,
.checkbox-ui .d-block.checkbox-item
{
    width: 14px;
    height: 14px;
    border: 1px solid #7f8aa8;
    border-radius: 2px;
    margin: 0 25px 0 0;
    vertical-align: top;
    border: 1px solid #00104b;
    position: relative;
    overflow: hidden;
}

.checkbox-ui div.checkbox-item:after,
.checkbox-ui .d-block.checkbox-item:after
{
    width: 14px;
    height: 14px;
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
    margin: auto;
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: 12px 12px;
    transition: all .2s ease;
    display: block;
    content: "";
    transform: translateY(30px)
}

.more-button {
