#login-modal .login-header {
    font-size: 23px;
    font-weight: 500;
    margin: 45px 0 10px;
    color: #00104b
}

#login-modal input {
    margin: 0 0 10px;
    border: 1px solid #cfdcfe;
    line-height: 37px;
    height: 37px;
    padding: 0 15px;
    width: 203px;
    display: block;
}

#login-modal input[type="submit"] {
    border: none;
    background: #d5afc3;
    width: 235px;
    padding: 0;
    line-height: 39px;
    height: 39px;
    font-size: 14px;
    font-weight: 500;
    color: #fff;
    cursor: pointer;
    transition: all .2s ease-in;
}

#login-modal input[type="submit"]:hover {
    background: #e01d7b
}

#login-modal .checkbox-ui {
    margin: 10px 0;
}

#login-modal .checkbox-ui div,
#login-modal .checkbox-ui span 
{
    font-size: 12px;
}
#login-modal .checkbox-ui div.checkbox-item,
#login-modal .checkbox-ui .d-block.checkbox-item
{
    margin: 0 10px 0 0
}

#login-modal .checkbox-ui input {
    display: none
}

#login-modal .close-modal {
    display: block;
    position: absolute;
    right: 20px;
    top: 20px;
    width: 15px;
    height: 15px;
    background: url("i/close-popup.png") no-repeat center center, transparent;
    border: none;
}

#login-modal .close-modal:hover {
    background: url("i/close-popup-pink.png") no-repeat center center, transparent
}

@media screen and (max-width:767px) {
    #call-you-modal {
        overflow: auto;
        width: auto;
        height: auto;
        background: #fff;
    }
    #call-you-modal form {
        height: 200px;
        position: absolute;
        top: 0;
        bottom: 0;
        margin: auto;
        left: 0;
        right: 0
    }
    #call-you-modal .call-you-header {
        font-size: 23px;
        font-weight: 500;
        margin: 0 auto 10px;
        width: 280px
    }
    #call-you-modal input {
        margin: 0 auto 10px;
        width: 248px;
    }
    #call-you-modal input[type="submit"] {
        margin: 20px auto;
        width: 280px
    }
    #login-modal {
        overflow: auto;
        width: auto;
        height: 100vh;
        overflow-x: auto;
        top: 0;
    }
    #login-modal .login-left {
        position: absolute;
        top: 0;
        right: 0;
        left: 0;
        bottom: 0;
        width: 280px;
        margin: auto auto auto -40px;
        width: auto;
        height: 270px
    }
    #login-modal .login-right {
        display: none;
    }
    #login-modal .checkbox-ui {
        width: 230px;
        margin: 0 auto;
        display: block
    }
    #login-modal .login-header {
        width: 230px;
        margin: 0 auto 10px
    }
    #login-modal input {
        margin: 0 auto 10px;
        width: 198px;
    }
    #login-modal .link {
        margin: 10px 20px;
    }
    #login-modal input[type="submit"] {
        margin: 20px auto;
        width: 230px
    }
}

@media screen and (max-width:1230px) {
    #content .content-header {
        margin: 0 20px 30px;
        font-size: 26px;
    }
    #content .content-header:after {
        height: 3px
    }
    #content-text {
        margin: 0 20px 0 0;
    }
    #content-text h1 {
        font-size: 26px;
    }
}

@media screen and (max-width:1230px) {
    .content-search input {
        line-height: 28px;
        font-size: 12px;
        margin: 0 0 0 20px
    }
    .content-search .count-search {
        font-size: 12px;
        margin: 10px 0 20px 20px
    }

    #sidebar.right-sidebar {
        margin: 0 20px 0 0
    }
    #sidebar #sidebar-filter {
        padding: 15px;
    }
    #sidebar #sidebar-filter form {
        background: #fff;
        padding: 15px 15px 15px;
        font-size: 10px;
    }
    #sidebar #sidebar-filter form h2 {
        font-size: 15px
    }
    #sidebar #sidebar-filter form h3 {
        font-size: 12px;
        color: #00104b;
        margin: 0 0 12px
    }
    #sidebar #sidebar-filter form #sidebar-search {
        font-size: 10px;
        line-height: 20px
    }
    #sidebar #sidebar-filter form .checkbox-ui div,
    #sidebar #sidebar-filter form .checkbox-ui .d-block
    {
        font-size: 12px;
    }
    #sidebar #sidebar-filter form .checkbox-ui div.checkbox-item,
    #sidebar #sidebar-filter form .checkbox-ui .d-block.checkbox-item
    {
        margin: 0 20px 0 0;
        width: 12px;
        height: 12px
    }
    #sidebar #sidebar-filter form .price-from {
        width: calc(50% - 10px);
    }
    #sidebar #sidebar-filter form .price-from input {
        line-height: 20px
    }
    #sidebar #sidebar-filter form .price-to {
        width: calc(50% - 10px);
    }
    #sidebar #sidebar-filter form .price-to input {
        line-height: 20px
    }
    #sidebar #sidebar-filter form #reet-filter {
        font-size: 15px;
        width: calc(100% + 30px);
        margin: 20px -15px 0
    }
    #sidebar #sidebar-one-levlel ul li {
        display: block;
        font-size: 12px;
        font-weight: 500;
    }
    #sidebar #sidebar-one-levlel ul li a {
        line-height: 30px;
        padding: 0 25px;
        border-bottom: 4px solid #fff
    }
}

@media screen and (max-width:767px) {
    .content-search {
        text-align: center;
    }
    .content-search input {
        width: 218px;
        margin: 0
    }
}
@media screen and (max-width:767px){
    #content .content-header {
        margin: 30px 20px;
    }
    #header #mid-header #mid-nav-block {
        position: static;
        z-index: 3;
    }
}
@media screen and (max-width:667px){
    #footer .middle-footer #footer-tel a {
        font-size: 24px;
    }
}
@media screen and (max-width: 568px){
    .category-card {
        width: 100%;
        padding-top: 50%;
    }
}
@media screen and (max-width:480px){
    #header #top-header .center{
        align-items: flex-start;
    }
    #header #top-header .phone-box {
        justify-content: flex-start;
        align-items: flex-start;
        flex-direction: column;
        margin-bottom: 40px;
    }
    #header #top-header #connect-button {
        line-height: 20px;
        margin-left: 20px;
    }
    #header #mid-header #cart-like-block {
        position: absolute;
        left: auto;
        top: -40px;
        right: 0;
    }
    #header #mid-header #mid-nav-block #search-button {
        left: calc(100% - 160px);
        top: -50px;
    }
    #header #mid-header #mid-nav-block #search-form input {
        top: -90px;
        right: auto;
        left: 20px;
        width: calc(100% - 160px);
    }
    #header #mid-header #cart-like-block {
        width: 75px;
    }
    #footer .top-footer {
        padding: 20px 0 0;
    }
    #footer .middle-footer .center{
        flex-direction: column;
        align-items: flex-start;
    }
    #footer .middle-footer #footer-tel a {
        font-size: 28px;
    }
    #footer .middle-footer #footer-call-you, 
    #footer #footer-social{
        font-size: 18px;
    }
    .social-icons a {
        font-size: 26px;
        width: 40px;
        height: 40px;
        margin-right: 10px;
    }
}
@media screen and (max-width:374px){
    #header #top-header #header-tel a {
        margin-right: 10px;
        margin-left: 10px;
        font-size: 14px;
    }
    #header #top-header #connect-button {
        margin-left: 10px;
    }
}
