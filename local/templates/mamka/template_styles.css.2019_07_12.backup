    }
    /*----------------/footer-----------------*/
}

@media screen and (max-width:1023px) {
    /*----------------header-----------------*/
    #header #top-header #top-nav {
        display: none;
    }
    #header #top-header #login-button {
        margin: 0 20px 0 0
    }
    #header #top-header #header-tel a {
        margin-left: 20px;
    }
    #header #mid-header #logo {
        margin: 10px 0 10px 20px;
        width: 190px
    }
    #header #mid-header #mid-nav-block {
        width: calc(100% - 210px - 143px - 25px - 20px);
        margin: 0;
    }
    #header #mid-header #menu-button {
        display: block;
        width: 25px;
        height: 25px;
        margin: 0;
        border: none;
        background: transparent;
        position: relative;
        z-index: 4;
        position: relative;
    }
    #header #mid-header #menu-button span {
        display: block;
        position: absolute;
        left: 0;
        width: 25px;
        height: 2px;
        background: #00104b;
        transition: all .2s ease;
    }
    #header #mid-header #menu-button span.line1 {
        top: 2px
    }
    #header #mid-header #menu-button span.line2 {
        top: 12px;
