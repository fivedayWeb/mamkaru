.oplata-content-block h2,
.dostavka-block h2 {
    font-size: 32px;
    font-family: "FuturaRoundBold", "Roboto", sans-serif;
    color: #00104b;
    margin: 20px 0;
}
.oplata-content-block h3 {
    font-size: 18px;
    margin: 10px 0;
}
.oplata-accordeon ul {
    margin-left: 20px;
}
h3.oplata-accordeon-title span {
    background: #00104b;
    border-radius: 50px;
    padding: 5px 10px;
    color: white;
    margin-right: 15px;
}

h3.oplata-accordeon-title {
    cursor: pointer;
    font-size: 14px;
    margin: 15px 0;
}

h3.oplata-accordeon-title:hover span {
    background: #e01d7b;
}
.promo-block p,
.promo-block ul li,
.promo-block ol li,
table.oplata-content-table td{
    font-size: 14px;
    margin-bottom: 10px;
    text-align: justify;
}
.promo-block a{
    font-size: 14px;
    color: #000;
}
section.oplata-accordeon.active_block h3.oplata-accordeon-title span {
    background: #e01d7b;
}
.oplata-accordeon-content {
    margin-left: 45px;
}

.oplata-content-block ul {
    margin-left: 20px;
}
table.oplata-content-table {
    border-collapse: collapse;
    margin-bottom: 10px;
}
table.oplata-content-table td {
    border-bottom: 0;
    padding: 9px 15px;
    border: 1px solid #354c75;
    margin: 0 !important;
}

#all-cities {
    margin: 20px 0 0;
    display: none;
}

#all-cities .city-item {
    color: #00104b;
    float: left;
    width: calc(33.333333% - 40px);
    margin: 0 0 30px 30px;
}

#all-cities .city-item .city-header {
    font-size: 18px;
    margin: 0 0 5px 70px;
    font-weight: 300
}
@media screen and (max-width:1230px) {
    .oplata-content-block h2, .dostavka-block h2 {
    font-size: 26px;
    }
    #all-cities .city-item {
        color: #00104b;
        float: left;
        width: calc((100% - 20px) / 3 - 20px);
        margin: 0 0 30px 20px;
    }
    #all-cities .city-item .city-header {
        font-size: 14px;
        margin: 0 0 5px 70px;
        font-weight: 300
    }
}

body {
    background: #fff;
    font-family: "Roboto-Regular", sans-serif;
    font-size: 16px
}

input,
textarea,
keygen,
select,
button {
    font-family: "Roboto-Regular", sans-serif
}

a {
    text-decoration: none
}

img {
    max-width: 100%;
    height: auto
}

* {
    margin: 0;
    padding: 0
}

.clear:after {
    display: table;
    width: 100%;
    clear: both;
    content: ""
}
.flexbox-block{
    display: flex;
    justify-content: space-between;
}
.center,
.container
{
    width: 100%;
    max-width: 1230px;
    margin: 0 auto
}
.tab-content {
    display: none;
}

.tab-content.active {
    display: block
}

input:hover:not(:focus) {
    transition: all .2s ease-in;
    border-color: #e01d7b !important
}

input:focus {
    outline: #a5c7fe auto 1px
}
/*----------------header-----------------*/
#header {
    background: #fff;
}

#header #top-header {
    border-bottom: 1px solid #eaedf1;
}
#header #top-header #top-nav {
    font-size: 12px;
    line-height: 30px;
    font-weight: 300;
}

#header #top-header #top-nav li {
    display: inline-block;
    margin: 0 25px 0 0;
}

#header #top-header #top-nav li a {
    color: #344d76;
    transition: all .2s ease-in;
}

#header #top-header #top-nav li a:hover {
    color: #e01d7b;
}
#header #top-header .phone-box{
    justify-content: flex-end;
}

#header #top-header #login-button {
    background: #d5afc3;
    border: none;
    color: #fff;
    width: 131px;
    line-height: 30px;
    cursor: pointer;
    transition: all .2s ease-in;
    font-size: 12px;
    font-weight: 500;
}

#header #top-header #login-button:hover {
    background: #e01d7b
}

#header #top-header #connect-button {
    margin: 0 40px 0 0;
    border: none;
    background: transparent;
    color: #00104b;
    font-size: 12px;
    font-weight: 500;
    line-height: 30px;
    cursor: pointer;
}

#header #top-header #connect-button span {
    border-bottom: 1px dashed #00104b;
    transition: all .2s ease-in
}

#header #top-header #connect-button:hover span {
    border-bottom: 1px dashed #fff
}

#header-tel br {
    display: none;
}
#header #top-header #header-tel a {
    font-family: "FuturaRoundBold", "Roboto", sans-serif;
    color: #e01d7b;
    line-height: 30px;
    margin: 0 20px 0 0
}

#header #mid-header {
    border-bottom: 1px solid #eaedf1;
    position: relative;
}
#header #mid-header .center{
    align-items: center;
}
.logo {
    width: 12%;
    min-width: 190px;
}
.logo .link {
    display: inline-block;
    font-size: 36px;
    font-weight: 500;
    color: #00104b;
}
.logo .link span{
    color: #e01d7b;
}

#header #mid-header #menu-button {
    display: none;
    outline: none
}

#header #mid-header #mid-nav-block {
    width: 70%;
    position: relative;
}

#header #mid-header #mid-nav-block #main-nav {
    width: 100%;
    line-height: 56px;
    overflow: auto;
}

#header #mid-header #mid-nav-block #main-nav #mobile-connect-button {
    display: none
}
#header #mid-header #mid-nav-block #main-nav ul{
    display: flex;
    justify-content: flex-end;
}
#header #mid-header #mid-nav-block #main-nav ul li {
    display: inline-block;
    font-size: 12px;
    font-weight: 500;
    opacity: 1;
    transition: all .15s ease-in;
    position: relative;
}

#header #mid-header #mid-nav-block #main-nav ul li.removed {
    transform: translateY(-50px);
    opacity: 0
}

#header #mid-header #mid-nav-block #main-nav ul li a {
    color: #344d76;
    margin: 0 25px 0 0;
    transition: all .2s ease-in;
}

#header #mid-header #mid-nav-block #main-nav ul li a:hover {
    color: #e01d7b;
}

#header #mid-header #mid-nav-block #search-button {
    width: 40px;
    height: 40px;
    border: none;
    background: transparent;
    outline: none;
    position: relative;
    cursor: pointer;
}

#header #mid-header #mid-nav-block #search-button span {
    display: none;
}

#header #mid-header #mid-nav-block #search-button svg {
    stroke: #00104b;
    display: block;
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    margin: auto;
    transition: all .15s ease-in
}

#header #mid-header #mid-nav-block #search-button:hover svg {
    stroke: #e01d7b
}

#header #mid-header #mid-nav-block #search-button.active svg {
    width: 0;
    height: 0
}

#header #mid-header #mid-nav-block #search-button.active:hover:after {
    background: url("i/close-popup-pink.png") no-repeat center center, transparent
}

#header #mid-header #mid-nav-block #search-button.active:after {
    width: 100%;
    height: 100%
}

#header #mid-header #mid-nav-block #search-button:after {
    display: block;
    content: "";
    width: 0%;
    height: 0%;
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    margin: auto;
    transition: all .15s ease-in;
    background: url("i/close-popup.png") no-repeat center center
}

#header #mid-header #mid-nav-block #search-form input {
    position: absolute;
    padding: 0 10px;
    right: 50px;
    top: 0;
    line-height: 40px;
    height: 40px;
    width: calc(100% - 72px);
    border: 1px solid #7f8aa8;
    border-radius: 3px;
    bottom: 0;
    margin: auto;
    visibility: hidden;
    opacity: 0;
    transform: translateY(50px);
    transition: hidden .15s ease-in, opacity .15s ease-in, transform .15s ease-in;
}

#header #mid-header #mid-nav-block #search-form input.active {
    visibility: visible;
    opacity: 1;
    transform: translateY(0)
}

#header #mid-header #cart-like-block {
    width: 12%;
    justify-content: flex-end;
}

#header #mid-header #cart-like-block a {
    font-size: 12px;
    text-align: center;
    width: 50px;
    display: block;
    color: #344d76;
    font-weight: 500;
    transition: all .2s ease-in;
}

#header #mid-header #cart-like-block a:hover {
    color: #e01d7b
}

#header #mid-header #cart-like-block a span {
    display: block;
    margin: -5px 0 0
}

#header #mid-header #cart-like-block a svg {
    margin: 0 auto;
    display: block
}

#header #mid-header #cart-like-block a#like-header-link {
    width: 60px;
    margin: 0 10px 0 0;
}

#header #mid-header #cart-like-block a#like-header-link svg {
    stroke: #e01d7b;
    width: 42px;
    height: 42px
}

#header #mid-header #cart-like-block a#like-header-link span {
    margin: -7px 0 0
}

#header #mid-header #cart-like-block a#cart-header-link {
    position: relative;
}

#header #mid-header #cart-like-block a#cart-header-link #cart-count {
    position: absolute;
    border: 1px solid #78c479;
    background: #fff;
    color: #00104b;
    width: 15px;
    height: 15px;
    font-size: 10px;
    position: absolute;
    right: 10px;
    bottom: 13px;
    line-height: 15px;
    text-align: center;
    border-radius: 50%
}

#header #mid-header #cart-like-block a#cart-header-link svg {
    fill: #e01d7b;
    width: 40px;
    height: 40px;
    margin: -2px 0 2px 8px
}

#header #bottom-header a {
    display: block;
    width: calc(((100% - 30px) / 8));
    float: left;
    text-align: center;
    color: #00104b;
    font-size: 14px;
    font-weight: 500;
    padding: 0 0 10px;
    transition: all .2s ease-in;
}

#header #bottom-header a .header-icon {
    width: 55px;
    height: 55px;
    position: relative;
    margin: 10px auto -5px;
}

#header #bottom-header a .header-icon .header-icon-blue {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    transition: all .2s ease-in;
    opacity: 1
}

#header #bottom-header a .header-icon .header-icon-pink {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    transition: all .2s ease-in;
    opacity: 0
}

#header #bottom-header a span {
    display: block
}

#header #bottom-header a:hover,
#header #bottom-header a.active {
    color: #e01d7b;
}

#header #bottom-header a:hover .header-icon .header-icon-pink,
#header #bottom-header a.active .header-icon .header-icon-pink {
    opacity: 1
}

#header #bottom-header a:hover .header-icon .header-icon-blue,
#header #bottom-header a.active .header-icon .header-icon-blue {
    opacity: 0
}
/*----------------/header-----------------*/
/*----------------footer-----------------*/

#footer {
    margin: 30px 0 0;
    background: #e5e8ee;
}
#footer #footer-social{
    color: #00104b;
    font-size: 12px;
    font-weight: 500;
    margin: 15px 0 0;
    text-transform: uppercase;
}
#footer .top-footer {
    padding: 20px 0;
}
#footer .top-footer .flexbox-block{
    align-items: flex-start;
}

#footer #company-info {
    width: 25%;
}
#footer .middle-footer{
    padding-bottom: 20px;
}
#footer .middle-footer .center{
    align-items: flex-end;
}
#footer .middle-footer #footer-call-you {
    border: none;
    background: transparent;
    display: block;
    font-size: 12px;
    color: #00104b;
    font-weight: 500;
    text-align: left;
    border-bottom: 1px dashed #00104b;
    margin: 5px 0 0;
    cursor: pointer;
    transition: all .2s ease-in;
}

#footer .middle-footer #footer-call-you:hover {
    color: #e01d7b;
    border-bottom: 1px dashed #e01d7b
}

#footer-tel br {
    display: none;
}
#footer .middle-footer #footer-tel a {
    font-size: 28px;
    line-height: 29px;
    display: block;
    color: #00104b;
    font-weight: 500;
    transition: all .2s ease-in;
    margin-top: 10px;
}

#footer .middle-footer #footer-tel a:hover {
    color: #e01d7b
}
.social-icons{
    display: flex;
    justify-content: flex-start;
    margin-top: 10px;
}
.social-icons a{
    font-size: 18px;
    margin-right: 7px;
    width: 29px;
    height: 29px;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 2px;
    transition: 0.3s;
    background-color: #7f8aa8;
    color: #fff;
}
.social-icons a:hover{
    background-color: #e01d7b;
    color: #00104b;
    transition: 0.3s;
}
#footer #company-info #footer-social {
    color: #00104b;
    font-size: 12px;
    font-weight: 500;
    margin: 15px 0 0;
    text-transform: uppercase;
}

#footer #bottom-footer {
    background: #7f8aa8;
    padding: 10px 0;
    line-height: 30px;
    color: #fff;
    display: none;
}

#footer #bottom-footer a {
    color: #fff;
    text-decoration: underline;
    transition: all .2s ease-in;
}

#footer #bottom-footer a:hover {
    color: #e01d7b
}

#footer #bottom-footer .metrics {
    float: left;
    margin: 0 0 0 30px
}

#footer #bottom-footer .made-in {
    float: right;
    margin: 0 30px 0 30px;
    font-size: 14px;
    font-weight: 500;
    text-align: right
}
/*----------------/footer-----------------*/

@media screen and (max-width:1230px) {
    /*----------------header-----------------*/
    #header #top-header #top-nav {
        margin: 0 0 0 20px
    }
    #header #top-header #login-button {
        margin: 0 20px 0 0
    }
    #header #mid-header .logo {
    margin-left: 20px;
    }
    #header #mid-header #cart-like-block {
        margin: 0 20px 0 0;
    }
    #header #mid-header #cart-like-block #cart-header-link .cart-count {
        right: 0;
        bottom: 5px
    }
    #header #mid-header #mid-nav-block {
        margin: 0 20px 0 0;
        width: 600px;
        font-size: 10px;
    }
    #header #mid-header #mid-nav-block #main-nav ul li a {
        margin: 0 20px 0 0
    }
    #header #bottom-header a {
        font-size: 12px;
        width: calc((100% - 20px) / 8)
    }
    /*----------------/header-----------------*/
    /*----------------footer-----------------*/
    #footer #company-info {
        margin: 0 0 0 20px;
