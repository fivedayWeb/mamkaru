.card .card-tag {
    position: absolute;
    right: -12px;
    padding: 2px 12px 0 10px;
    line-height: 34px;
    height: 32px;
    font-family: "FuturaRoundBold", "Roboto", sans-serif;
}

.card .card-tag.new,
.card .card-tag.action,
.card .card-tag.discount {
    background: #ffd200;
    top: 168px;
    color: #d50000;
    text-transform: uppercase;
}

.card .card-tag.new:before,
.card .card-tag.action:before,
.card .card-tag.discount:before {
    top: 0;
    display: block;
    position: absolute;
    left: -20px;
    content: "";
    width: 0;
    height: 0;
    border-top: 17px solid #ffd200;
    border-left: 20px solid transparent;
    border-bottom: 17px solid #ffd200
}

.card .card-tag.new:after,
.card .card-tag.action:after,
.card .card-tag.discount:after {
    width: 0;
    height: 0;
    display: block;
    content: "";
    position: absolute;
    right: 0;
    top: 34px;
    border-top: 12px solid #c5a200;
    border-right: 12px solid transparent
}

.card .card-tag.action {
    background: #6079ea;
    color: #fff;
    top: 207px;
}

.card .card-tag.action:before {
    border-top: 17px solid #6079ea;
    border-bottom: 17px solid #6079ea
}

.card .card-tag.action:after {
    border-top: 12px solid #4156b5
}

.card .card-tag.discount {
    background: #d50000;
    color: #fff;
    text-transform: lowercase;
    top: 247px;
}

.card .card-tag.discount:before {
    border-top: 17px solid #d50000;
    border-bottom: 17px solid #d50000
}

.card .card-tag.discount:after {
    border-top: 12px solid #910202
}

